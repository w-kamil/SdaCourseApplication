<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hfad.sdacourseapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".drawapp.DrawAppActivity"
            android:label="@string/draw_app" />
        <activity
            android:name=".simpledrawgame.SimpleDrawingGameMainActivity"
            android:label="@string/draw_game" />
        <activity
            android:name=".gallery.GalleryActivity"
            android:label="@string/gallery" />
        <activity
            android:name=".listapp.ListAppActivity"
            android:label="@string/todo_list" />
        <activity android:name=".quiz.QuizActivity"
            android:label="@string/quiz"></activity>
        <activity android:name=".libraryapp.LibraryApp"
            android:label="@string/library"></activity>
    </application>

</manifest>